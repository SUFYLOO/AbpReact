    <PackageReference Include="Serilog.AspNetCore" Version="6.1.0" />
    <PackageReference Include="Serilog.Sinks.Async" Version="1.5.0" />
    <PackageReference Include="Serilog.Sinks.Seq" Version="5.2.2" />
    <PackageReference Include="Microsoft.AspNetCore.DataProtection.EntityFrameworkCore" Version="7.0.1" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.AspNetCore.Mvc" Version="7.0.0-rc.6" />
    <PackageReference Include="Volo.Abp.Autofac" Version="7.0.0-rc.6" />
    <PackageReference Include="Volo.Abp.AutoMapper" Version="7.0.0-rc.6" />
    <PackageReference Include="Volo.Abp.Swashbuckle" Version="7.0.0-rc.6" />
