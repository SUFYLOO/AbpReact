    <PackageReference Include="Volo.Abp.TenantManagement.Application" Version="7.0.0-rc.6" />
    <PackageReference Include="Volo.Abp.TenantManagement.EntityFrameworkCore" Version="7.0.0-rc.6" />
    <PackageReference Include="Volo.Abp.TenantManagement.HttpApi" Version="7.0.0-rc.6" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.FeatureManagement.Application" Version="7.0.0-rc.6" />
